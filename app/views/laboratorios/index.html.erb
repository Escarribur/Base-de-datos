<p id="notice"><%= notice %></p>

<h1 align="center">
<span class="label label-info">Lista Laboratorios</span>
</h1>

<table class="table table-bordered" >
  <thead>
    <tr>
      <th>Id sede</th>
      <th>Descripcion laboratorio</th>
      <th>Piso laboratorio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @laboratorios.each do |laboratorio| %>
      <tr>
        <td><%= laboratorio.id_sede %></td>
        <td><%= laboratorio.descripcion_laboratorio %></td>
        <td><%= laboratorio.piso_laboratorio %></td>
        <td><%= link_to 'Show', laboratorio %></td>
        <td><%= link_to 'Edit', edit_laboratorio_path(laboratorio) %></td>
        <td><%= link_to 'Destroy', laboratorio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Laboratorio', new_laboratorio_path %>
