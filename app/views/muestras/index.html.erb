<p id="notice"><%= notice %></p>

<h1 align="center">
<span class="label label-info">Lista Muestras</span>
</h1>

<table class="table table-bordered" >
  <thead>
    <tr>
      <th>Descripcion muestra</th>
      <th>Tipo muestra</th>
      <th>Tiempo muestra</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @muestras.each do |muestra| %>
      <tr>
        <td><%= muestra.descripcion_muestra %></td>
        <td><%= muestra.tipo_muestra %></td>
        <td><%= muestra.tiempo_muestra %></td>
        <td><%= link_to 'Show', muestra %></td>
        <td><%= link_to 'Edit', edit_muestra_path(muestra) %></td>
        <td><%= link_to 'Destroy', muestra, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Muestra', new_muestra_path %>
